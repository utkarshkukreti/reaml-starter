{
  "name": "reaml-starter",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "run-p start:bsb start:parcel",
    "start:bsb": "bsb -make-world -w",
    "start:parcel": "parcel -d .cache/dev src/index.html",
    "build": "run-s build:bsb build:parcel",
    "build:bsb": "bsb -make-world",
    "build:parcel": "parcel build src/index.html",
    "clean": "rm -rf .cache dist && bsb -clean-world"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reaml": "^0.6.0"
  },
  "devDependencies": {
    "bs-platform": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.4"
  }
}
